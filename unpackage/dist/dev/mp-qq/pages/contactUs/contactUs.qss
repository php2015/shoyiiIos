.wrap {
  padding: 24rpx;
}
.slot-btn {
  width: 690rpx;
  height: 388rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  background: #FFFFFF;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  position: absolute;
  left: 0;
  top: 0;
}
.slot-btn__hover {
  background-color: #ebecee;
}
.pre-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  position: absolute;
  left: 0;
  top: 0;
}
.pre-item {
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  height: 388rpx;
  overflow: hidden;
  position: relative;
  margin-bottom: 20rpx;
}
.u-upload {
  position: relative;
}
.u-upload .u-list-item {
  width: 690rpx !important;
  height: 388rpx !important;
  z-index: 100;
}
.slot-content {
  font-size: 28rpx;
  color: #606266;
  padding-left: 30rpx;
  text-align: left;
  padding: 30rpx 30rpx;
}
.slot-content uni-view {
  padding: 10rpx 0;
}
.icon {
  width: 1em;
  height: 1em;
  font-size: 48rpx;
  vertical-align: -0.15em;
  overflow: hidden;
}

