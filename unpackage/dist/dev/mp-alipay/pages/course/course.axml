<view class="course"><view class="listCount"><checkbox-group data-event-opts="{{[['change',[['checkboxChange',['$event']]]]]}}" onChange="__e"><label class="listItem" style="display:flex;align-items:center;" a:for="{{carArr}}" a:for-item="item" a:for-index="__i0__" a:key="value"><view class="checkBox"><checkbox hidden="{{!(item.whether)}}" style="transform:scale(0.9);" value="{{item.id}}" checked="{{item.checked}}"></checkbox><view hidden="{{!(!item.whether)}}" class="notGoods">无货</view></view><view class="itemShow" style="display:flex;"><view class="listImg"><image style="width:140rpx;height:140rpx;" src="{{item.src}}" mode="aspectFill"></image></view><view class="itemCont" style="display:flex;flex-direction:column;justify-content:space-around;"><view class="itemName">{{''+item.name+''}}</view><view class="itemGrade">品相优良<text class="aliIcon"></text></view><view class="itemPriceTool"><view class="price">{{'￥'+item.price+''}}</view><view class="itemTool"><text hidden="{{!(!item.whether)}}" class="toolTwo">到货提醒</text><text hidden="{{!(!item.whether)}}" class="toolOne">预定</text><text data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" hidden="{{!(item.whether)}}" class="toolOne" onTap="__e">锁定</text></view></view><view class="aliIcon"></view></view></view></label></checkbox-group></view><checkbox-group data-event-opts="{{[['change',[['changeBook',['$event']]]]]}}" onChange="__e"><label><checkbox style="transform:scale(0.9);" checked="{{allFlag.checked}}" value="{{allFlag.cb}}"></checkbox>全选</label></checkbox-group></view>