<view class="login"><view class="login-main"><view class="login-logo"></view><view><view class="title_tip"><text>注册</text></view><view class="tel_login"><text data-event-opts="{{[['tap',[['areaCode',['$event']]]]]}}" onTap="__e">{{areacode}}</text><uni-icons vue-id="9a7b6094-1" type="arrowdown" onVueInit="__l"></uni-icons><input type="number" maxlength="11" placeholder="请输入手机号码" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" onInput="__e"/></view><view class="tel_login"><input type="number" maxlength="11" placeholder="请输入验证码" data-event-opts="{{[['input',[['__set_model',['','vertifyCode','$event',[]]]]]]}}" value="{{vertifyCode}}" onInput="__e"/><button class="codeimg" style="{{(styleObject)}}" type="default" disabled="{{disabled}}" data-event-opts="{{[['tap',[['getCode']]]]}}" catchTap="__e">{{getCodeText}}</button></view><input class="tel_login" type="password" placeholder="设置密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" onInput="__e"/><button class="getcodeBtn" type="default" hover-class="button-hover" data-event-opts="{{[['tap',[['register']]]]}}" catchTap="__e">注册</button></view></view><view class="footer"><checkbox-group name data-event-opts="{{[['change',[['select',['$event']]]]]}}" onChange="__e"><label class="label"><checkbox style="transform:scale(0.7);" checked="{{check}}" color="#86B0D4"></checkbox><text>我已阅读并同意</text><navigator data-event-opts="{{[['tap',[['EnterUseragree',['$event']]]]]}}" class="user" catchTap="__e">《用户协议》</navigator><text>和</text><navigator data-event-opts="{{[['tap',[['EnterSecrete',['$event']]]]]}}" class="secret" catchTap="__e">《隐私政策》</navigator></label></checkbox-group></view></view>